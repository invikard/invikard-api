{
    "version": 2,
    "name": "invikard-api",
    "alias": "invikard-api",
    "builds": [
      {
        "src": "index.js",
        "use": "@now/node-server@canary",
        "config": {
          "bundle": false,
          "maxLambdaSize": "50mb"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    }
}
  